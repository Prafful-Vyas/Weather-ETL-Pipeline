   "source": [
    "import duckdb\n",
    "\n",
    "con = duckdb.connect()\n",
    "\n",
    "con.execute(\"\"\"\n",
    "    CREATE OR REPLACE VIEW weather_raw AS\n",
    "    SELECT *\n",
    "    FROM 'data/**/*.parquet'\n",
    "\"\"\")"
   ]
  },
